{
  "name": "vue-sidebar-bootstrap",
  "version": "1.0.18",
  "description": "Create a mobile friendly sidebar layout for Bootstrap Vue.",
  "author": "Daniel Kiss",
  "keywords": [
    "sidebar",
    "vue-sidebar",
    "bootstrap-vue-sidebar",
    "sidebar-bootstrap-vue",
    "vue-sidebar-bootstrap",
    "bootstrap sidebar",
    "bootstrap layout",
    "vue bootstrap layout",
    "vue bootstrap"
  ],
  "license": "MIT",
  "main": "dist/vue-sidebar-bootstrap.ssr.js",
  "browser": "dist/vue-sidebar-bootstrap.esm.js",
  "module": "dist/vue-sidebar-bootstrap.esm.js",
  "unpkg": "dist/vue-sidebar-bootstrap.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "serve": "vue-cli-service serve demo/main.js",
    "build:demo": "vue-cli-service build demo/main.js",
    "lint": "vue-cli-service lint",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "publish:package": "npm run build && npm publish"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "bootstrap-vue": "^2.18.1",
    "core-js": "^3.6.5",
    "vue": "^2.6.12",
    "vue-router": "^3.4.8"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^1.19.1",
    "vue-template-compiler": "^2.6.11",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "vue": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "engines": {
    "node": ">=10"
  }
}
